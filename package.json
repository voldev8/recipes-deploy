{
  "name": "recipes-deploy-master",
  "version": "2.0.1",
  "description": "flavorites app",
  "scripts": {
    "server": "cd server && npm run nodemon",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run serve-heroku && cd ../server && npm install"
  },
